{% extends 'layout.html.twig' %}

{% block content %}
    <h1>Partenaire {{ partner.firstname }} {{ partner.lastname }}</h1>
        <table class="table">
			<thead>
				<tr>
					<th>Id du partenaire</th>
					<th>Prénom</th>
					<th>Nom</th>
                    <th>Adresse</th>
                    <th>E-mail</th>
                    <th>Téléphone</th>
                    <th>Description</th>
				</tr>
			</thead>
			<tbody>

                <tr>
                    <td> {{ partner.id }} </td>
                    <td> {{ partner.firstname }} </td>
                    <td> {{ partner.lastname }} </td>
                    <td> {{ partner.address }} </td>
                    <td> {{ partner.email }} </td>
                    <td> {{ partner.phone }} </td>
                    <td> {{ partner.description }} </td>
                </tr>

			</tbody>
		</table>
    <a href="/partner">Back</a>
{% endblock %}
